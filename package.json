{
  "name": "pathways",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "babel ./app -d build",
    "build:watch": "babel ./app -d build --watch",
    "build:client": "webpack --config ./webpack.prod.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "npm run build && npm run build:client",
    "start": "npm run build:prod && NODE_ENV=production node ./build/server/index.js",
    "start:dev": "parallelshell 'npm run build:watch' 'npm run build:watch:client' 'nodemon ./build/server/index.js'",
    "test": "jest --watch --coverage"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "testPathIgnorePatterns": [
      "/build/"
    ],
    "modulePathIgnorePatterns": [
      "/build/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.18.9",
    "parallelshell": "^3.0.2",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "supertest": "^3.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
